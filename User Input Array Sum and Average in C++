#include<iostream>
using namespace std;
int main(){
    int sum=0;
    int averg{};
    int arr[5];
    cout<<"Enter array if five number: "<<endl;
    // loop for take data from user;
    for(int i=0;i<5;i++){
        cin>>arr[i];
    }
    for(int i=0;i<5;i++){
        sum=sum+arr[i];
    }
    averg=sum/5;
    cout<<"The sum of all array elememnt: "<<sum<<endl;
    cout<<"The average of array: "<<averg<<endl;
}
