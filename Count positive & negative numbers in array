#include <iostream>
using namespace std;
int main(){
    int arr[5]={1,67,-3,5};
        for(int i=0;i<5;i++){
              if(arr[i]>0){
                     cout<<"POSITIVE: "<<arr[i]<<endl;
              }
              else if(arr[i]<0){
                     cout<<"NEGATIVE: "<<arr[i]<<endl;
              }
              else{
                     cout<<"ZERO: "<<arr[i]<<endl;
              }
        }   
}
